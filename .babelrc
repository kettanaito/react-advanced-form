{
    "presets": ["env", "react"],
    "plugins": [
        "transform-export-extensions",
        "transform-class-properties",
        "transform-object-rest-spread",
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true
        }]
    ],
    "env": {
        "production": {
            "presets": [
                ["env", {
                    "modules": false,
                    "targets": {
                        "browsers": "last 2 versions"
                    }
                }],
                "react"
            ],
            "plugins": [
                "transform-export-extensions",
                "transform-class-properties",
                "transform-object-rest-spread",
                ["transform-runtime", {
                    "helpers": false,
                    "polyfill": false,
                    "regenerator": true
                }]
            ]
        }
    }
}
