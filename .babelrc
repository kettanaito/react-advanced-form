{
    "presets": [
        "env",
        "stage-2",
        "react"
    ],
    "plugins": [
        "transform-export-extensions",
        "transform-class-properties",
        "transform-object-rest-spread"
    ],
    "env": {
        "production": {
            "presets": [
                ["env", {
                    "modules": false,
                    "targets": "last 2 versions"
                }],
                "react"
            ],
            "plugins": [
                "transform-export-extensions",
                "transform-class-properties",
                "transform-object-rest-spread",
                ["transform-runtime", {
                    "helpers": false,
                    "polyfill": false,
                    "regenerator": true
                }]
            ]
        }
    }
}
