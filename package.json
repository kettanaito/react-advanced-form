    "fields group",
    "validation",
    "form validation",
    "field validation",
    "async validation",
    "multi-level validation"
  ],
  "engines": {
    "node": ">=6"
  },
  "module": "src/index.js",
  "main": "lib/index.js",
  "scripts": {
    "start": "start-storybook -p 6009",
    "cypress": "node_modules/.bin/cypress open",
    "build": "NODE_ENV=production BABEL_ENV=production webpack",
